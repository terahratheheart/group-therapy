{
    "main": "node_modules/expo/AppEntry.js",
    "scripts": {
        "start": "expo start",
        "android": "expo start --android",
        "ios": "expo start --ios",
        "web": "expo start --web",
        "eject": "expo eject",
        "typecheck": "tsc",
        "format:check": "npx prettier --check \"**/*.{js,jsx,json,ts,tsx}\"",
        "format": "npx prettier --write \"**/*.{js,jsx,json,ts,tsx}\"",
        "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\""
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run typecheck && lint-staged"
        }
    },
    "lint-staged": {
        "src/**/*.{js,jsx,ts,tsx}": "eslint",
        "**/*.{js,jsx,json,ts,tsx}": "prettier --write"
    },
    "dependencies": {
        "@expo-google-fonts/cutive-mono": "^0.2.0",
        "@expo-google-fonts/delius-unicase": "^0.2.0",
        "@react-native-community/masked-view": "0.1.10",
        "@react-navigation/drawer": "^6.0.1",
        "@react-navigation/native": "^6.0.0",
        "@react-navigation/stack": "^5.14.5",
        "expo": "~42.0.1",
        "expo-app-loading": "^1.1.2",
        "expo-av": "~9.2.3",
        "expo-font": "~9.2.1",
        "expo-linear-gradient": "~9.2.0",
        "expo-status-bar": "~1.0.4",
        "react": "^16.13.1",
        "react-dom": "16.13.1",
        "react-native": "https://github.com/expo/react-native/archive/sdk-42.0.0.tar.gz",
        "react-native-countdown-circle-timer": "^2.5.3",
        "react-native-elements": "^3.4.2",
        "react-native-gesture-handler": "~1.10.2",
        "react-native-reanimated": "~2.2.0",
        "react-native-restart": "^0.0.22",
        "react-native-safe-area-context": "3.2.0",
        "react-native-screens": "~3.4.0",
        "react-native-svg": "^12.1.1",
        "react-native-web": "~0.13.12",
        "react-redux": "^7.2.4",
        "redux": "^4.1.0",
        "status-bar": "^2.0.3"
    },
    "devDependencies": {
        "@babel/core": "^7.9.0",
        "@types/react": "~16.9.35",
        "@types/react-native": "~0.63.2",
        "@types/react-native-restart": "^0.0.14",
        "@typescript-eslint/eslint-plugin": "^4.28.4",
        "@typescript-eslint/parser": "^4.28.4",
        "babel-plugin-module-resolver": "^3.2.0",
        "eslint": "^7.31.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-react": "^7.24.0",
        "husky": "^4.3.8",
        "i": "^0.3.6",
        "lint-staged": "^11.1.1",
        "prettier": "2.3.2",
        "typescript": "~4.0.0"
    },
    "private": true
}
